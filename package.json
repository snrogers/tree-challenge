    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.52",
    "@babel/preset-react": "^7.0.0-beta.52",
    "@babel/runtime": "^7.0.0-beta.52",
    "babel-loader": "8.0.0-beta.4",
    "browser-sync": "^2.24.5",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "express": "^4.16.3",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-bootstrap4-modal": "^1.4.0",
    "react-dom": "^16.4.1",
    "react-popper": "^1.0.0-beta.6",
    "react-redux": "^5.0.7",
    "reactstrap": "^6.3.0",
    "redux": "^4.0.0",
    "sequelize": "^4.38.0",
    "webpack": "^4.15.1",
    "webpack-node-externals": "^1.7.2",
    "ws": "^5.2.1"
  },
  "scripts": {
    "build": "webpack",
    "db-create": "sequelize db:create --config app/db/config/config.js; sequelize db:migrate --migrations-path app/db/migrations/ --config app/db/config/config.js",
    "dev": "FORCE_COLOR=1 nf start dev-transpile,dev-test-client,dev-test-server,dev-serve",
    "dev-serve": "NODE_ENV=development nodemon app/build/server/server.js",
    "dev-test-server": "NODE_ENV=development mocha-webpack \"app/test/server\" ==recursive --webpack-config webpack.server.js --watch",
    "dev-test-client": "NODE_ENV=development mocha-webpack \"app/test/client\" ==recursive --webpack-config webpack.client.js --watch",
    "dev-transpile": "NODE_ENV=development webpack --watch",
    "postinstall": "nf start build,db-create,db-migrate",
    "start": "node app/build/server.js",
    "watch": "webpack --watch"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.0.0-beta.52",
    "chai": "^4.1.2",
    "foreman": "^3.0.1",
    "mocha": "^5.2.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "nodemon": "^1.17.5",
